#include<iostream>
#include<bits/stdc++.h>
#include<cstdio>

#define ll long long
#define endl '\n'

using namespace std;
int main(){
    ios::sync_with_stdio(false);
    cin.tie(NULL);
    
    int n; cin>>n;
    
    for(int i=1; i<=n;i++){
        ll k2 = i*i;
        ll ai = (k2*(k2-1))/2;
        ll bi = 4*(i-1)*(i-2);
        
        ll ans = ai - bi;
        cout<<ans<<endl;
    }
}
