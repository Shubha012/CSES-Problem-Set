#include <iostream>
#include <vector>

#define ll long long
#define endl '\n'
#define MOD 1000000007

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin>>n;
    
    long multiple = 5;
    long ans = 0;
    
    for(int i=1; i<=12; i++){
        ans = ans+n/multiple;
        multiple *= 5;
    }
    cout<<ans;
    return 0;
}
